%h1 My DnD
-if current_user.has_role?(:master)
  %h2= "Master: #{@username}"
  %h3 Active sessions
  -@activesessions.each do |session|
    %h3= link_to "Id session: #{session.id}", session_path(session.id)
-elsif current_user.has_role?(:player)
  %h2= "Player: #{@username}"
  %h3 Active sessions
  -@activesessions.each do |session|
    %h3= link_to "Id session: #{session.id}", session_path(session.id)
  =form_tag destroy_user_session_path, :method => :get do
    =submit_tag 'Logout'
